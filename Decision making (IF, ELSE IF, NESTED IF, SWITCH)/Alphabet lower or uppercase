#include<stdio.h>
main()
{
	char ch;
	printf("Enter a character:-");
	scanf("%c", &ch);
	
	if(ch >='a' && ch <='z')
	{
		printf("'%c' is a lowercase alphabet.",ch);
	}
	else if(ch >='A' && ch <='Z')
	{
		printf("'%c'character is a uppercase alphabet.",ch);
	}
	else
	{
		printf("'%c'this is not alphabet");
	}
	
}
