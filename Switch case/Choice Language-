#include <stdio.h>
main() 
{
        int num;
        printf("Enter Your Choice\n");
        printf("Press 1 For 'c'\n");
        printf("Press 2 For 'c#'\n");
        printf("Press 3 For 'c++'\n");
        printf("Press 4 For 'JAVA'\n");
        printf("Press 5 For 'PYTHON' :-\n");
        scanf("%d",&num);
        
        switch (num)
	   {
            case 1:
                printf("WELCOME TO 'C' LANGUAGE");
                break;
            case 2:
                 printf("WELCOME TO 'C#' LANGUAGE");
                break;
            case 3:
                  printf("WELCOME TO 'C++' LANGUAGE");
                break;
             case 4:
                  printf("WELCOME TO 'JAVA' LANGUAGE");
                break;
             case 5:
                  printf("WELCOME TO 'PYTHON' LANGUAGE");
                break;
             default:
                printf("INVALID CHOICE!!!");
                break;
        }
     
}
